<%- products.each do |p| %>
    <tr>
        <td class="text-left py-3 px-4">
            <%= image_tag p.first_photo_url(:thumb) %>
        </td>
        <td class="w-1/3 text-left py-3 px-4">
            <%= p.sku %>
        </td>
        <td class="w-1/3 text-left py-3 px-4">
            <%= p.name %>
        </td>
        <td class="text-left py-3 px-4">
            <%= p.count %>
        </td>
        
    </tr>
<% end %>